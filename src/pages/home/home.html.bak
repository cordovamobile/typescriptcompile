<ion-header>
  <ion-navbar>
    <button ion-button menuToggle>
      <ion-icon name="menu"></ion-icon>
    </button>
    
	<!--ion-title>Home</ion-title-->
	<img src="assets/imgs/logos/logo_top_menu.png" style="height: 44px;" />
	
	
  </ion-navbar>
</ion-header>

<ion-content padding>

	<ion-grid>
		<ion-row>
			<ion-col>
				<ion-card>
					<img src="assets/imgs/home/expert_forum.png" />
				</ion-card>
			</ion-col>
			<ion-col>
				<ion-card (click)="openPage('NewsPage')">
					<img src="assets/imgs/home/latest_news.png" />
				</ion-card>
			</ion-col>
		</ion-row>
		<ion-row>
			<ion-col>
				<ion-card>
					<img src="assets/imgs/home/resources.png" />
				</ion-card>
			</ion-col>
			<ion-col>
				<ion-card>
					<img src="assets/imgs/home/utilities.png" />
				</ion-card>
			</ion-col>
		</ion-row>
		<ion-row>
			<ion-col>
				<ion-card>
					<img src="assets/imgs/home/expenses.png" />
				</ion-card>
			</ion-col>
			<ion-col>
				<ion-card (click)="openPage('CaClientDashboardPage')">
					<img src="assets/imgs/home/invoice_management.png" />
				</ion-card>
			</ion-col>
		</ion-row>
	</ion-grid>
	
	
</ion-content>
