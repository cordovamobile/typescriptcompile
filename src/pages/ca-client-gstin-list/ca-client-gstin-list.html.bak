<ion-header>
	<ion-navbar>
		<button ion-button menuToggle>
			<ion-icon name="menu"></ion-icon>
		</button>
		<ion-title>{{client.business_name}} GSTINs</ion-title>
	</ion-navbar>
</ion-header>


<ion-content padding>
	
	<ion-list>
		
		<ion-item *ngFor="let g of gstins">
			{{g.gstin}}
			<a (click)="edit_gstin()" style="float: right;">
				<ion-icon name="create"></ion-icon>
				Edit
			</a>
		</ion-item>
	
	</ion-list>
	
</ion-content>
